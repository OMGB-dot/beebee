local L = loadstring(game:HttpGet("https://sirius.menu/rayfield"))()

local WN = L:CreateWindow({
    Name = "WINTER TIME!!!",
    LoadingTitle = "Loading...",
    LoadingSubtitle = "Winter tp gui",
})

local TP = WN:CreateTab("TP", 4483362458)
local misc = WN:CreateTab("Misc", 4483362458)

local LP = game:GetService("Players").LocalPlayer
local INFO = WN:CreateTab("Info", 4483362458)


local function TPto(pos)
    local char = LP.Character
    if char and char:FindFirstChild("HumanoidRootPart") then
        char.HumanoidRootPart.CFrame = CFrame.new(pos)
    end
end

TP:CreateButton({
    Name = "Snowland",
    Callback = function()
        TPto(Vector3.new(0, 0, 0)) 
    end
})

TP:CreateButton({
    Name = "Iceland",
    Callback = function()
        TPto(Vector3.new(-4221, 2, 13))
    end
})

TP:CreateButton({
    Name = "Grassland",
    Callback = function()
        TPto(Vector3.new(-12345, 4, 438)) 
    end
})

TP:CreateButton({
    Name = "Grassland VendingMachine",
    Callback = function()
        TPto(Vector3.new(-13577, 4, -605)) 
    end
})

TP:CreateButton({
    Name = "Grassland",
    Callback = function()
        TPto(Vector3.new(-12845, 4, 173)) 
    end
})

TP:CreateButton({
    Name = "Tahiti",
    Callback = function()
        TPto(Vector3.new(3574, 40, 125)) 
    end
})

TP:CreateButton({
    Name = "The moon",
    Callback = function()
        TPto(Vector3.new(-1, 3059, -2)) 
    end
})

TP:CreateButton({
    Name = "Mars",
    Callback = function()
        TPto(Vector3.new(59, 46028, 8299)) 
    end
})

misc:CreateButton({
    Name = "Kill Ronin",
    Callback = function()
        local ronin = workspace:FindFirstChild("ronin")
        if ronin and ronin:FindFirstChild("Humanoid") then
            ronin.Humanoid.Health = 0
        end
    end
})

INFO:CreateParagraph({
    Title = "wINETER TELEPORTER GUI",
    Content = "This GUI lets you teleport to lands"
})

INFO:CreateParagraph({
    Title = "There was no scripts for winter ",
    Content = "so i made it myself"
})

INFO:CreateParagraph({
    Title = "To be able to use the kill ronin button",
    Content = "make sure ronin is following you, or else it wont work"
})


INFO:CreateParagraph({
    Title = "Made by",
    Content = "someone :)"
})

L:LoadConfiguration()
